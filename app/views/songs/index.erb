<a href="/">Home</a>
<h1>Songs</h1>
<hr/>

<% @songs.each do |song| %>
  <div class="song">
    <h3><a href="/songs/<%= song.id %>"><%= song.title %></a> <small>(by: <%= song.artist %>)</small></h3>
    <p>
      <a href="<%= song.url %>"><%= song.url %></a>
    </p>
  </div>
<% end %>

<hr/>
<a href="/songs/new">Add a song</a>